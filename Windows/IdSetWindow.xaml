<Window x:Class="PhysicsEngine.Windows.IdSetWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PhysicsEngine.Windows"
        mc:Ignorable="d"
        Title="IDの設定" Height="90" Width="350">
    <Grid Margin="10">
        <StackPanel Orientation="Horizontal">
            <TextBox x:Name="IdSetTextBox" Width="250" Height="30" VerticalContentAlignment="Center" KeyDown="IdSetTextBox_KeyDown"/>
            <Button Content="設定" Width="50" Height="30" Margin="10,0,0,0" Click="IdSetButton_Click" Background="#FF8C8C8C" Foreground="White" BorderBrush="{x:Null}"/>
        </StackPanel>
    </Grid>
</Window>
